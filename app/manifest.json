{
    "name": "Write Wall",
    "version": "2.0.1",
    "manifest_version": 2,
    "incognito": "split",
    "description": "A simple, sync-able text pad similar to Write Space",
    "icons": {
        "16": "images/icon-16.png",
        "19": "images/icon.png",
        "48": "images/icon-48.png",
        "64": "images/icon-64.png",
        "128": "images/icon-128.png",
        "512": "images/icon-512.png"
    },
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-nNGvbNOXxvM4cXS9511XXfSv1uzm+VgcO7aozPcZqfE=' https://www.googletagmanager.com https://www.google-analytics.com; object-src 'self'",
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}