{
  "preset": "ts-jest/presets/default-esm",
  "testEnvironment": "node",
  "clearMocks": true,
  "coverageProvider": "v8",
  "extensionsToTreatAsEsm": [
    ".ts"
  ],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "^.+\\.[tj]sx?$": [
      "ts-jest",
      {
        "useESM": true,
        "tsconfig": "tsconfig.test.json"
      }
    ]
  }
}
